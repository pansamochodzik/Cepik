<table>
  <thead>
    <td>Name</td>
    <td>Colour</td>
    <td>Vin number</td>
    <td>License plate</td>
    <td>Production at</td>
    <td>Registration at</td>
    <td>Action</td>
  </thead>
  <tbody>
    <td><%= @car.name %></td>
    <td><%= @car.colour %></td>
    <td><%= @car.vin_number %></td>
    <td><%= @car.license_plate %></td>
    <td><%= @car.production_at %></td>
    <td><%= @car.registration_at %></td>
    <td><%= link_to 'Back', :back %> | <%= link_to 'delete', car_path(@car), method: :delete %> | <%= link_to 'edit', edit_car_path(@car) %> </td>
  </tbody>
</table>
